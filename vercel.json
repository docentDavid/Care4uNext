{
    "version": 2,
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}